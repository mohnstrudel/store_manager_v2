.preloadable-img__container data-controller="preloadable-img" data-preloadable-img-src-value=src
  img.preloadable-img__img data-preloadable-img-target="img" class=class_names style="width: #{width}px;"
  .preloadable-img__skeleton.visible data-preloadable-img-target="skeleton" style="width: #{width}px; height: #{width * 1.2}px;"