tr id="#{dom_id version}"
  td = version.id
  td = version.value
  td = format_time version.created_at
  td = format_time version.updated_at
  - if action_name != "show" 
    td.actions
      = link_to version
        i.icn 📄
        | Show
      = link_to edit_version_path(version)
        i.icn ✏️
        | Edit
