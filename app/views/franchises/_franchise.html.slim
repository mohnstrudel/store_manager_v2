tr id="#{dom_id franchise}"
  td
    == franchise.id
  td
    == franchise.title
  td
    == franchise.created_at
  td
    == franchise.updated_at
  - if action_name != "show" 
    td
      == link_to franchise
        <i class='icn'>📄</i> Show
      == link_to edit_franchise_path(franchise)
        <i class='icn'>✏️</i> Edit